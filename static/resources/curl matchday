--header 'Content-Type: application/json' \
--data '{
    "number": 12,
    "topPlayer": "Lionel Messi",
    "lastPlayer": "Cristiano Ronaldo",
    "secondToLast": "Neymar Jr",
    "noSubs": false,
    "accumulated": 30
  }'